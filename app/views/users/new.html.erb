<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    
	<form accept-charset="UTF-8" action="signup" method="POST" >
	
		<input type="hidden" name="authenticity_token" value ="<%= form_authenticity_token %>" >
		 
	    <%= render "form" %>
	</form>
    

     
  </div>
</div>
